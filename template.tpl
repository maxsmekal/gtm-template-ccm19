// Enter your template code here.
const log = require('logToConsole');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const makeInteger = require('makeInteger');
const gtagSet = require('gtagSet');
//const setInWindow = require('setInWindow');

const injectScript = require('injectScript');
const encodeUriComponent = require('encodeUriComponent');
const queryPermission = require('queryPermission');
let ccm19_cdn = data.ccm19_cdn ? data.ccm19_cdn : "cloud.ccm19.de";
const ccm19_apikey = data.ccm19_apikey;
const ccm19_domain = data.ccm19_domain;
const gcm_defaults = data.defaultSettings;

gtagSet('developer_id.dNGQxZW', true);

if(typeof(ccm19_cdn) == 'string' && ccm19_cdn.substring(0,8) == 'https://')
{
  ccm19_cdn = ccm19_cdn.substring(8,9999);
}

let scriptUrl = null;
if(ccm19_apikey && ccm19_cdn)
{
  if (ccm19_domain) {
    scriptUrl = 'https://'+ encodeUriComponent(ccm19_cdn) +'/app.js?apiKey='+ encodeUriComponent(ccm19_apikey) + '&domain=' + encodeUriComponent(ccm19_domain);
  } else {
    scriptUrl = 'https://'+ encodeUriComponent(ccm19_cdn) +'/app.js?apiKey='+ encodeUriComponent(ccm19_apikey);
  }

}
const splitInput = (input) => { return input.split(',').map(entry => entry.trim()).filter(entry => entry.length !== 0); };


if (gcm_defaults && typeof gcm_defaults === 'object' && gcm_defaults.length !== undefined)
{
  const setDefaultConsentState = require('setDefaultConsentState');
  gcm_defaults.forEach(settings => {
    const regions = splitInput(settings.region);
    let consentState = {
      wait_for_update: 2000,
    };
    if (regions.length > 0) {
      consentState.region = regions;
    }

    for (let key in settings) {
      if (key != 'region' && settings[key] != '') {
        consentState[key] = settings[key];
      }
    }
    setDefaultConsentState(consentState);
  });
}

if (queryPermission('access_globals', 'write', "_ccm19gtm_gcmDefaultsConfigured")) {
  setInWindow("_ccm19gtm_gcmDefaultsConfigured", true, false);
}

if (data.ads_data_redaction) {
  gtagSet('ads_data_redaction', true);
}
if (data.url_passthrough) {
  gtagSet('url_passthrough', true);
}

const ccmIsLoaded = queryPermission('access_globals', 'read', "CCM.version") ? copyFromWindow("CCM.version") !== undefined : null;

if (scriptUrl) {
  if (ccmIsLoaded) {
    log('CCM19 is directly loaded into website. Skipping script injection.');
    data.gtmOnSuccess();
  } else {
    log('URL: ', scriptUrl);
    if (queryPermission('inject_script', scriptUrl)) {
      injectScript(scriptUrl, data.gtmOnSuccess, () => {
        log('Script injection failed.');
        data.gtmOnFailure();
      });
    } else {
      log('Permission denied for script injection.');
      data.gtmOnFailure();
    }
  }
} else {
  if (!ccmIsLoaded) {
    log('CCM19 is not directly loaded into website and no API key is configured!');
  }
  data.gtmOnSuccess();
}


